{
	"name": "Nimbl Candidate Extension V2",
	"description": "Extension for scrape candidates profiles from linkedin",
	"version": "1.0.0",
	"key": "",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [{
		"matches": [
			"https://www.linkedin.com/*"
		],
		"css": [
			"styles/content.css",
			"styles/bootstrap.min.css"
		],
		"js": [
			"content.js"
		]
	}],
	"permissions": [
		"identity",
		"identity.email",
		"storage"
	],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "imgs/logo16.png",
			"48": "imgs/logo32.png",
			"128": "imgs/logo120.png"
		}
	},
	"icons": {
		"16": "imgs/logo16.png",
		"48": "imgs/logo32.png",
		"128": "imgs/logo120.png"
	}
}